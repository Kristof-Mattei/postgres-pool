{
  "name": "postgres-pool",
  "version": "1.3.0",
  "description": "Connection pool implementation for node-pg",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run lint && npm run test",
    "pretest": "npm run build",
    "test": "mocha --reporter spec",
    "lint": "tslint --fix -p tsconfig.lint.json -c tslint.json -t stylish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpirik/postgres-pool.git"
  },
  "keywords": [
    "node",
    "postgres",
    "pg",
    "pool",
    "connection",
    "pooling"
  ],
  "author": "Jim Geurts <jim@biacreations.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpirik/postgres-pool/issues"
  },
  "homepage": "https://github.com/mpirik/postgres-pool#readme",
  "dependencies": {
    "pg": "^7.x.x",
    "strict-event-emitter-types": "~2.0.0",
    "uuid": "^3.x.x"
  },
  "engines": {
    "node": ">=8.11"
  },
  "devDependencies": {
    "@types/chai": "~4.1.7",
    "@types/chai-as-promised": "~7.1.0",
    "@types/faker": "~4.1.5",
    "@types/mocha": "~5.2.6",
    "@types/pg": "^7.x.x",
    "@types/sinon": "~7.0.11",
    "@types/uuid": "~3.4.4",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "faker": "~4.1.0",
    "husky": "1.3.1",
    "mocha": "6.0.2",
    "sinon": "7.3.1",
    "tslint": "5.15.0",
    "tslint-config-airbnb": "~5.11.0",
    "typescript": "~3.4.2"
  }
}
